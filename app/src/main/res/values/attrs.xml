<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WeekdayCircleView">
        <attr name="weekday" format="enum">
            <enum name="sunday" value="0"/>
            <enum name="monday" value="1"/>
            <enum name="tuesday" value="2"/>
            <enum name="wednesday" value="3"/>
            <enum name="thursday" value="4"/>
            <enum name="friday" value="5"/>
            <enum name="saturday" value="6"/>
        </attr>
        <attr name="selected" format="boolean" />
    </declare-styleable>

    <declare-styleable name="NumberScroller">
        <attr name="max_value" format="integer" />
        <attr name="min_value" format="integer" />
        <attr name="step" format="integer" />
        <attr name="digits" format="integer" />
</declare-styleable>
</resources>
